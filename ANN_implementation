   "source": [
    "#initializing the ann\n",
    "ann = tf.keras.models.Sequential() #initializes ann into an instance of a sequence\n",
    "\n",
    "#adding input layer and first hidden layer\n",
    "ann.add(tf.keras.layers.Dense(units=6,activation='relu')) #dense class creates hidden layer using add method and relu is rectifier and units= 6 is number of neurons in the hidden layer\n",
    "\n",
    "#adding the second hidden layer\n",
    "ann.add(tf.keras.layers.Dense(units=6,activation='relu'))\n",
    "\n",
    "#adding the output layer\n",
    "ann.add(tf.keras.layers.Dense(units=1,activation='sigmoid')) #unit 1 because only binary prediction"
   ]
  },
